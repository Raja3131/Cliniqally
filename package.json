{
  "name": "cliniqally-backend",
  "version": "1.0.0",
  "description": "This holds the cliniqally backend application code",
  "main": "cliniqally-backend.js",
  "scripts": {
    "start": "npm run clean:dist&&tsc -b --pretty --verbose&&cpy --cwd=src --parents \"**/*\" \"!**/*.ts\" ../dist/&&cpy --rename=.env src/environments/.env dist/environments/ && node dist/cliniqally-backend.js",
    "start:beta": "npm run clean:dist&&tsc -b --pretty --verbose&&cpy --cwd=src --parents \"**/*\" \"!**/*.ts\" ../dist/&&cpy --rename=.env src/environments/.beta.env dist/environments/ && node dist/cliniqally-backend.js",
    "start:qa": "npm run clean:dist&&tsc -b --pretty --verbose&&cpy --cwd=src --parents \"**/*\" \"!**/*.ts\" ../dist/&&cpy --rename=.env src/environments/.qa.env dist/environments/ && node dist/cliniqally-backend.js",
    "start:prod": "npm run clean:dist&&tsc -b --pretty --verbose&&cpy --cwd=src --parents \"**/*\" \"!**/*.ts\" ../dist/&&cpy --rename=.env src/environments/.prod.env dist/environments/ && node dist/cliniqally-backend.js",
    "build": "npm run clean:dist&&tsc -b --pretty --verbose&&cpy --cwd=src --parents \"**/*\" \"!**/*.ts\" ../dist/&&cpy --rename=.env src/environments/.env dist/environments/",
    "build:beta": "npm run clean:dist&&tsc -b --pretty --verbose&&cpy --cwd=src --parents \"**/*\" \"!**/*.ts\" ../dist/&&cpy --rename=.env src/environments/.beta.env dist/environments/",
    "build:qa": "npm run clean:dist&&tsc -b --pretty --verbose&&cpy --cwd=src --parents \"**/*\" \"!**/*.ts\" ../dist/&&cpy --rename=.env src/environments/.qa.env dist/environments/",
    "build:prod": "npm run clean:dist&&tsc -b --pretty --verbose&&cpy --cwd=src --parents \"**/*\" \"!**/*.ts\" ../dist/&&cpy --rename=.env src/environments/.prod.env dist/environments/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:dist": "rimraf dist",
    "tsc": "tsc -b",
    "tsn": "ts-node",
    "dev": "ts-node-dev --respawn ./src/cliniqally-backend.ts",
    "dev:beta": "cross-env-shell NODE_ENV=beta ts-node-dev --respawn ./src/cliniqally-backend.ts",
    "dev:qa": "cross-env-shell NODE_ENV=qa ts-node-dev --respawn ./src/cliniqally-backend.ts",
    "dev:prod": "cross-env-shell NODE_ENV=prod ts-node-dev --respawn ./src/cliniqally-backend.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "prepush": "npm run lint:fix",
    "migrate-create": "node-pg-migrate -m src/migrations/ -j ts create",
    "migrate-ts": "npm run build && cross-env-shell DATABASE_URL=postgres://cliniqally:EXMJPHdmyAhqvUX2gv8vANAz4C42mTJg@localhost:5432/cliniqally node-pg-migrate -m ./dist/migrations --envPath src/environments/.env up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alokraj68/cliniqally-backend.git"
  },
  "author": "alokraj68",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alokraj68/cliniqally-backend/issues"
  },
  "homepage": "https://github.com/alokraj68/cliniqally-backend#readme",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cpy-cli": "^3.1.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "express": "^4.17.3",
    "express-basic-auth": "^1.2.1",
    "express-validator": "^6.14.0",
    "ioredis": "^5.0.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.3.8",
    "node-pg-migrate": "^6.2.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.3.0",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/pg": "^8.6.5",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8"
  }
}
